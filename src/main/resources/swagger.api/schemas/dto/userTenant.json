{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "This schema is used to work with the User Tenant association",
  "UserTenant": {
    "type": "object",
    "properties": {
      "id": {
        "type": "string",
        "format": "uuid"
      },
      "userId": {
        "type": "string",
        "format": "uuid"
      },
      "username": {
        "type": "string"
      },
      "tenantId": {
        "type": "string"
      },
      "tenantName": {
        "type": "string"
      },
      "isPrimary": {
        "type": "boolean"
      }
    },
    "additionalProperties": false,
    "required": [
      "id",
      "userId",
      "username",
      "tenantId",
      "isPrimary"
    ]
  }
}
