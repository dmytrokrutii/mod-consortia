{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "Collection of tenants",
  "TenantCollection": {
    "type": "object",
    "properties": {
      "tenants": {
        "type": "array",
        "description": "Tenants",
        "items": {
          "type": "object",
          "$ref": "tenant.json#/Tenant"
        }
      },
      "totalRecords": {
        "type": "integer"
      }
    },
    "additionalProperties": false,
    "required": [
      "tenants",
      "totalRecords"
    ]
  }
}
