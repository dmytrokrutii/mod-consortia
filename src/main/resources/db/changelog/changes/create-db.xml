<?xml version="1.1" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                   http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">

  <changeSet id="MODFISTO-394@@create-tenant-table" author="singhadesh">
    <createTable tableName="tenant">

      <column name="tenant_id" type="varchar(50)">
        <constraints primaryKey="true" primaryKeyName="pk_tenant_id" nullable="false"/>
      </column>

      <column name="tenant_name" type="varchar(100)">
        <constraints unique="true" nullable="false"/>
      </column>

    </createTable>
  </changeSet>

</databaseChangeLog>
